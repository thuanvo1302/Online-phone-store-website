                                                        <input type="text" class="form-control" id="uname"
                                                            value="{{user.username}}" name="uname">
                                                    </div>
                                                    <div class="form-group col-sm-6">
                                                        <label for="email">Email:</label>
                                                        <input type="text" class="form-control" id="email"
                                                            value="{{user.email}}" name="email">
                                                    </div>
                                                    <div id="checkrole" class="form-group col-sm-6">
                                                        <label>User Role:</label>

                                                        <select id="selectedRole" name="role"
                                                            class="selectpicker form-control" data-style="py-0">
                                                            <option value="">Select</option>
                                                            <option value="admin">Admin</option>
                                                            <option value="saleperson">Sale person</option>
                                                        </select>
                                                    </div>
                                                    <div class="form-group col-sm-6">
                                                        <label for="fullname">Fullname:</label>
                                                        <input class="form-control" id="fullname"
                                                            value="{{user.fullName}}" name="fname">
                                                    </div>





                                                </div>
                                                <div id="liveAlertPlaceholder"></div>

                                                <button type="submit" class="btn btn-primary mr-2"
                                                    id="liveAlertBtn">Submit</button>
                                                <button type="reset" class="btn iq-bg-danger">Cancel</button>
                                            </form>
                                            <script>
                                                const alertPlaceholder = document.getElementById('liveAlertPlaceholder')
                                                const appendAlert = (message, type) => {
                                                    const wrapper = document.createElement('div')
                                                    if (type == 'success') {
                                                        wrapper.innerHTML = [
                                                            `<svg xmlns="http://www.w3.org/2000/svg" class="d-none">
                                                            <symbol id="info-fill" viewBox="0 0 16 16">
                                                                <path
                                                                    d="M8 16A8 8 0 1 0 8 0a8 8 0 0 0 0 16zm.93-9.412-1 4.705c-.07.34.029.533.304.533.194 0 .487-.07.686-.246l-.088.416c-.287.346-.92.598-1.465.598-.703 0-1.002-.422-.808-1.319l.738-3.468c.064-.293.006-.399-.287-.47l-.451-.081.082-.381 2.29-.287zM8 5.5a1 1 0 1 1 0-2 1 1 0 0 1 0 2z" />
                                                            </symbol>
                                                        </svg>
                                                        <div class="alert alert-success d-flex align-items-center fade show" role="alert">
                                                            <svg class="bi flex-shrink-0 me-2" role="img" aria-label="Info:" width="15px" height="15px"><use xlink:href="#info-fill"/></svg>
                                                            <div>
                                                                ${message}
                                                            </div>
                                                        </div>`
                                                        ].join('')
                                                    }
                                                    else if (type == 'failed') {
                                                        wrapper.innerHTML = [
                                                            `<svg xmlns="http://www.w3.org/2000/svg" class="d-none">
                                                            <symbol id="exclamation-triangle-fill" viewBox="0 0 16 16">
                                                                <path d="M8.982 1.566a1.13 1.13 0 0 0-1.96 0L.165 13.233c-.457.778.091 1.767.98 1.767h13.713c.889 0 1.438-.99.98-1.767L8.982 1.566zM8 5c.535 0 .954.462.9.995l-.35 3.507a.552.552 0 0 1-1.1 0L7.1 5.995A.905.905 0 0 1 8 5zm.002 6a1 1 0 1 1 0 2 1 1 0 0 1 0-2z"/>
                                                            </symbol>
                                                        </svg>
                                                        <div class="alert alert-danger d-flex align-items-center" role="alert">
                                                            <svg class="bi flex-shrink-0 me-2" role="img" aria-label="Danger:" width="15px" height="15px"><use xlink:href="#exclamation-triangle-fill"/></svg>
                                                            <div>
                                                                ${message}
                                                            </div>
                                                        </div>`
                                                        ].join('')
                                                    }
                                                    else {
                                                        alertPlaceholder.textContent = ''
                                                    }

                                                    alertPlaceholder.append(wrapper)
                                                }
                                                const alertTrigger = document.getElementById('liveAlertBtn')

                                                var userRole = "{{user.role}}";

                                                var selectedRoleDropdown = document.getElementById('selectedRole');
                                                for (var i = 0; i < selectedRoleDropdown.options.length; i++) {
                                                    if (selectedRoleDropdown.options[i].value === userRole) {
                                                        selectedRoleDropdown.options[i].selected = true;
                                                        break;
                                                    }
                                                }
                                                $('#btnUpload').on('change', function (e) {
                                                    $('#profilePicture').attr('src', URL.createObjectURL(e.target.files[0]))
                                                })

                                                $('#form-per_info').on('submit', (e) => {
                                                    e.preventDefault()

                                                    if ($('#btnUpload').prop('files').length == 0) {
                                                        $('#image_error').text('Please choose a px`icture to set avatar')
                                                    }
                                                    else {
                                                        $('#image_error').text('')
                                                        $('#role').val($('#selectedRole').val())

                                                        var formData = new FormData(e.target);
                                                        axios({
                                                            method: 'put',
                                                            url: '/users/update',
                                                            data: formData

                                                        })
                                                            .then((res) => {

                                                                if (alertTrigger) {
                                                                    appendAlert('', 'undefined')
                                                                    appendAlert(res.data.message, 'success')
                                                                }
                                                                setTimeout(() => {
                                                                    alertPlaceholder.textContent = ''
                                                                }, 2000)
                                                            })
                                                            .catch((error) => {
                                                                console.error("AxiosError:", error);
                                                                if (error.response) {

                                                                    console.error("Response data:", error.response.data);
                                                                    console.error("Response status:", error.response.status);
                                                                    console.error("Response headers:", error.response.headers);
                                                                } else if (error.request) {
                                                                    console.error("Request data:", error.request);
                                                                } else {
                                                                    console.error("Error message:", error.message);
                                                                }
                                                                if (alertTrigger) {
                                                                    appendAlert('', 'undefined')
                                                                    appendAlert(error.response.data.message, 'failed')
                                                                }
                                                            });

                                                    }
                                                }) 
                                            </script>
                                        </div>
                                    </div>
                                </div>
